<?xml version="1.1" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="helloWorld" class="com.tutorialspoint.HelloWorld">
		<property name="message" value="Hello World!" />
		<property name="messageNumber" value="3" />
	</bean>

	<bean id="address" class="com.tutorialspoint.Address">
		<constructor-arg value="Mariana Trench" type="String"></constructor-arg>
		<constructor-arg value="2001" type="int"></constructor-arg>
	</bean>

	<bean id="employee" class="com.tutorialspoint.Employee">
		<constructor-arg value="Steffen Dworsky" type="String">
		</constructor-arg>
		<constructor-arg value="404" type="int">
		</constructor-arg>
		<constructor-arg>
			<ref bean="address" />
		</constructor-arg>
	</bean>
</beans>